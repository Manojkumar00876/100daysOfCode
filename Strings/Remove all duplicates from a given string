class Solution {
    String removeDuplicates(String str) {
        
    LinkedHashSet<Character> set = new LinkedHashSet<>();
    for(int i=0; i<str.length(); i++) {
        set.add(str.charAt(i));
    }
    
    StringBuilder sb = new StringBuilder();
    
   for( char ch : set){
       sb.append(ch);
   }
   
   return sb.toString();
    }
}

