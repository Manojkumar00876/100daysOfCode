class Solution {
    public int getCommon(int[] nums1, int[] nums2) {

         int min = Integer.MAX_VALUE; 
        HashSet <Integer> set = new HashSet<>();
        for(int x : nums1){
            if(!set.contains(x)){
                set.add(x);
            }
        }
        for(int x : nums2){
        if(set.contains(x)){
        min = Math.min(min,x);
          }
        }

       if(min == Integer.MAX_VALUE ){
           return -1;
       }

        return min;
    }
}
